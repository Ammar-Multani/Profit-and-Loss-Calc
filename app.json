{
  "expo": {
    "name": "P&L Calc",
    "slug": "profit-loss-calculator",
    "scheme": "profitlosscalc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "jsEngine": "hermes",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.enacton.profitlosscalc",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "UIBackgroundModes": [],
        "CFBundleAllowMixedLocalizations": true,
        "NSCameraUsageDescription": "This app uses the camera to share screenshots of your calculations.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to save and share calculation screenshots."
      },
      "icon": "./assets/icon.png",
      "splash": {
        "image": "./assets/splash-icon-light.png",
        "resizeMode": "contain",
        "backgroundColor": "#ecedee",
        "dark": {
          "image": "./assets/splash-icon-dark.png",
          "backgroundColor": "#151718"
        }
      }
    },
    "android": {
      "icon": "./assets/icon.png",
      "package": "com.enacton.profitlosscalc",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-light.png",
        "monochromeImage": "./assets/adaptive-icon-dark.png",
        "backgroundColor": "#ffffff"
      },
      "allowBackup": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash-icon-light.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ecedee",
          "dark": {
            "image": "./assets/splash-icon-dark.png",
            "backgroundColor": "#151718"
          }
        }
      ]
    ],
    "eas": {
      "projectId": "10c80020-2c06-41e3-9fff-6558b4ebd0c5"
    }
  }
}
